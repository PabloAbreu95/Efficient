<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pablo.efficient">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DisciplinaPackage.add_disciplina"
            android:label="@string/title_activity_add_disciplina"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DisciplinaPackage.DisciplinaActivity"
            android:label="@string/title_activity_disciplina"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DisciplinaPackage.edit_disciplina"
            android:label="@string/title_activity_edit_disciplina"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HorarioPackage.horariosActivity"
            android:label="@string/title_activity_horarios"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HorarioPackage.add_Horario"
            android:label="@string/title_activity_add__horario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DiasSemanaPackage.DiasSemanaActivity"
            android:label="@string/title_activity_dias_semana"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>